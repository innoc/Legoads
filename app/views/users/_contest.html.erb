<% @beep=Beep.find(feed.beep_id)%>
<% @beep2=Beep.find(feed.source_id)%>
  <div class="beep_master_blog">
    <div id="image_tag">
     <%=link_to image_tag("contest_default.png")%>
    </div>

    <div class="beep_contain_block">

        <div id="beep_user_names">

        <span style="color:purple">Contest Alert!!! </span>
           
        </div>
        <div id="beed_content">
          <%=@beep.beep_content%>
        </div>
          <br/>
          &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;vs<br/><br/>
        <div class="show_beep">
          <%=@beep2.beep_content%>
        </div>
      <div class="smiley_set">
      <div class="created_at">
       <span class="beep_created">
         <%= distance_of_time_in_words_to_now(feed.created_at)%></span>
      </div>

     </div>

    </div>
    <div class="see_all">
      <span class="smiley_set">
      <%=link_to "Join Contest",{:controller=>"compete",:action=>'compete_room',:id=>feed.compete_id}%>
      </span>

    </div>



  </div>














